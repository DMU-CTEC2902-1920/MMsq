@model IEnumerable<MMSQ.Models.clsLeagueTable>
@{
    ViewBag.Title = "LeagueList";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <!-- CSS Links-->
    @Styles.Render("~/Content/MMSQ.css")
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Game League List</title>
</head>
<body>

    <!-- League List -->
    <table class="table" style="text-align:center; position:absolute; left:-30px; top:130px; width:1000px; height:300px;">
        description
        <tr style="text-align:center;">
            <th>
                Rank ID
            </th>
            <th>
                Game Title
            </th>
        </tr>


        @foreach (var item in Model)
        {
            <tr>
                <td>
                    1st
                </td>
                <td>
                    Call Of Duty MW
                </td>
            </tr>
            <tr>
                <td>
                    2nd
                </td>
                <td>
                    Minecraft
                </td>
            </tr>
        }
    </table>
    </body>
</html>
